stoptime=5.0
music_timestep = 0.001
[sensor]
  binary=./ros_sensor_adapter
  args=
  np=1
  ros_topic=/jubot/laserscan
  message_type=Laserscan
  sensor_update_rate=30
[encoder]
  binary=./nef_encoder
  args=
  np=1
[nest]
  binary=./dummy_network.py
  args=
  np=1
[decoder]
  binary=./linear_readout_decoder
  args=
  np=1
  tau=0.03
[command]
  binary=./ros_command_adapter
  args=
  np=1
  ros_topic=/jubot/cmd_vel
  message_type=Twist
  angular.z=1
  command_rate=10
sensor.out->encoder.in[10]
encoder.out->nest.in[200]
nest.out->decoder.in[200]
decoder.out->command.in[2]
